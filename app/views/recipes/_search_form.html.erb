    <%= search_form_for @q, url: url do |f| %>
        <div class="d-flex  w-auto input-group mb-3">
          <%= f.search_field :title, class: 'form-control' %>
          <div class="input-group-append">
            <%= f.submit class: 'btn btn-outline-success' %>
          </div>
        </div>
    <% end %>
